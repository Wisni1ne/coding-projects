<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bracketmanager">

    <application
        android:name=".main.MainApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".activities.SplashActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".front.FrontMain"></activity>

        <activity android:name=".tourney.TourneyMain"></activity>
        <activity android:name=".tourney.TourneyNew"></activity>
        <activity android:name=".tourney.TourneyLoad"></activity>
        <activity android:name=".tourney.TourneyMatch"></activity>

        <activity android:name=".match.MatchListActivity"></activity>
        <activity android:name=".name.NameListActivity"></activity>

    </application>

</manifest>